<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.ui.view" id="view_languages_tree">
        <field name="name">Languages tree</field>
        <field name="model">adm_uni.languages</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    
    <record model="ir.ui.view" id="view_language_levels_tree">
        <field name="name">Language levels tree</field>
        <field name="model">adm_uni.languages.level</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

</odoo>